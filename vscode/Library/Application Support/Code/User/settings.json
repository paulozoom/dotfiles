{
  "[markdown]": {
    "editor.defaultFormatter": "darkriszty.markdown-table-prettify",
    "files.trimTrailingWhitespace": false
  },
  "bracketPairColorizer.showBracketsInGutter": true,
  "breadcrumbs.enabled": true,
  "css.validate": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "IBM Plex Mono",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.formatOnSave": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.scale": 2,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fit",
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "trailing",
  "editor.showFoldingControls": "mouseover",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 120,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/.jekyll-cache": true,
    "**/.jekyll-metadata": true
  },
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "gitblame.ignoreWhitespace": true,
  "gitblame.statusBarMessageFormat": "${author.name} · ${time.ago} · ${commit.summary}",
  "githubIssues.ignoreUserCompletionTrigger": ["python", "ruby"],
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:updated-desc"
    }
  ],
  "importCost.debug": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "less.validate": false,
  "liquid.format": true,
  "markdown.preview.breaks": true,
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.folders.theme": "classic",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.opacity": 1,
  "material-icon-theme.saturation": 1,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "ruby.lint": {
    "reek": true
  },
  "ruby.rubocop.useBundler": true,
  "ruby.useBundler": true,
  "scm.diffDecorationsGutterWidth": 2,
  "scss.validate": false,
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "svg.preview.mode": "svg",
  "task.problemMatchers.neverPrompt": {
    "rake": true
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.1,
  "todo-tree.general.tags": ["TODO", "FIXME", "QUESTION"],
  "todo-tree.highlights.enabled": false,
  "todo-tree.tree.showScanModeButton": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.autoDetectColorScheme": true,
  "window.nativeTabs": true,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "yaml.format.enable": false,
  "zenMode.hideTabs": false,
  "bracket-pair-colorizer-2.colors": ["Gold", "Orchid", "LightSkyBlue"],
  "editor.matchBrackets": "never",
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.showBracketsInRuler": true,
  "files.associations": {
    "*.html": "html"
  },
  "editor.suggest.showStatusBar": true,
  "editor.linkedEditing": true,
  "search.mode": "reuseEditor",
  "marksToOutline.mark": "MARK: ",
  "markJump.maximumLimit": -1,
  "scss.showErrors": true,
  "editor.rulers": [
    120
  ],
  "githubPullRequests.fileListLayout": "tree",
  "redhat.telemetry.enabled": false,
  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false
}
